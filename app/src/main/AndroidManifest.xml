        <activity android:name=".View.Activities.ContactActivity">
        </activity>
        <activity android:name=".View.Activities.ChatRoomActivity">
        </activity>
        <activity android:name=".View.Activities.ProfileActivity">
        </activity>
    </application>

</manifest>